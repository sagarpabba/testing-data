<?xml version="1.0" encoding="UTF-8"?>
<?page id="p1" title="Test Data Spreadsheet Management"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<zk xmlns:w="http://www.zkoss.org/2005/zk/client">
	<window id="mainWin" hflex="1" vflex="1" apply="org.zkoss.zss.app.ui.AppCtrl">
		<vlayout hflex="1" vflex="1" spacing="0">
			<include hflex="1" src="~./zssapp/mainMenubar.zul" />
			<spreadsheet id="ss" 
				hflex="1" vflex="1" 
				showToolbar="true" showFormulabar="true" showSheetbar="true" showContextMenu="true"
				maxrows="200" maxcolumns="40"/>
		</vlayout>
	</window>
</zk>